{
  "manifest_version": 3,
  "name": "ArchiveClick",
  "short_name": "Archiver",
  "description": "Opens Link in archive.today",
  "version": "0.0.1",
  "minimum_chrome_version": "88",
  "permissions": [
    "contextMenus", 
    "tabs"],
  "icons": {
    "16": "assets/archive16.png",
    "32": "assets/archive32.png",
    "48": "assets/archive48.png",
    "128": "assets/archive128.png"
  },
  "action": {
    "default_icon": {
      "16": "assets/archive16.png",
      "24": "assets/archive24.png",
      "32": "assets/archive32.png"
    },
    "default_title": "ArchiveClick"
  },
  "background": {"service_worker": "main.js"}
}
